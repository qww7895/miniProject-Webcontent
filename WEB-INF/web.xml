<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>Member_Board</display-name>

<!-- Controller  -->
	<servlet>
		<servlet-name>MainFrontController</servlet-name>
		<servlet-class>
			net.main.action.MainFrontController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>MagFrontController</servlet-name>
		<servlet-class>net.magazine.action.MagFrontController</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>PurchaseFrontController</servlet-name>
		<servlet-class>net.purchase.action.PurchaseFrontController</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>ProductFrontController</servlet-name>
		<servlet-class>net.product.action.ProductFrontController</servlet-class>
	</servlet>	
<!--  -->	
	<servlet>
		<servlet-name>PromotionFrontController</servlet-name>
		<servlet-class>net.promotion.action.PromotionFrontController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ShopFrontController</servlet-name>
		<servlet-class>net.shop.action.ShopFrontController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>SsoFrontController</servlet-name>
		<servlet-class>net.sso.action.SsoFrontController</servlet-class>
	</servlet>		
	<servlet>
		<servlet-name>FoundationFrontController</servlet-name>
		<servlet-class>net.foundation.action.FoundationFrontController</servlet-class>
	</servlet>			
	
	<servlet>
    <servlet-name>MemberFrontController</servlet-name>
    <servlet-class>paris.member.action.MemberFrontController</servlet-class>
	  </servlet>
	<servlet>
    <servlet-name>AdminFrontController</servlet-name>
    <servlet-class>net.admin.action.AdminFrontController</servlet-class>
	  </servlet>	
			
			
<!-- Mapping  -->
	<servlet-mapping>
		<servlet-name>MainFrontController</servlet-name>
		<url-pattern>*.gg</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>MagFrontController</servlet-name>
		<url-pattern>*.mag</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>PurchaseFrontController</servlet-name>
		<url-pattern>*.pc</url-pattern>
	</servlet-mapping>	
	<servlet-mapping>
		<servlet-name>ProductFrontController</servlet-name>
		<url-pattern>*.pb</url-pattern>
	</servlet-mapping>	
  	<!--  -->
	<servlet-mapping>
		<servlet-name>PromotionFrontController</servlet-name>
		<url-pattern>*.pr</url-pattern>
	</servlet-mapping>	  
	<servlet-mapping>
		<servlet-name>ShopFrontController</servlet-name>
		<url-pattern>*.sp</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>SsoFrontController</servlet-name>
		<url-pattern>*.in</url-pattern>
	</servlet-mapping>	 
	<servlet-mapping>
		<servlet-name>FoundationFrontController</servlet-name>
		<url-pattern>*.fd</url-pattern>
	</servlet-mapping>		 
   <servlet-mapping>
    <servlet-name>MemberFrontController</servlet-name>
    <url-pattern>*.log</url-pattern>
	 </servlet-mapping>	 
   <servlet-mapping>
    <servlet-name>AdminFrontController</servlet-name>
    <url-pattern>*.ad</url-pattern>
	 </servlet-mapping>	 
	 
	 
	 
	 
	 
	 
	 
	 
	 
	 
	 
	 
	 
	 
	 
	  
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  



	<display-name>
	Paris</display-name>	
	
	<resource-ref>
		<description>Connection</description>
		<res-ref-name>jdbc/OracleDB</res-ref-name>
		<res-type>javax.sql.DataSource</res-type>
		<res-auth>Container</res-auth>
	</resource-ref>
</web-app>